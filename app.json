{
  "expo": {
    "name": "Divvy",
    "slug": "divvy",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo-app.png",
    "scheme": "demoapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo-app.png",
        "backgroundColor": "#FFF4B6"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.tuanemtramtinh.divvy"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo-app.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#FFF4B6",
          "dark": {
            "backgroundColor": "#FFF4B6"
          }
        }
      ],
      "expo-font",
      "expo-web-browser",
      [
        "@sentry/react-native/expo",
        {
          "url": "https://sentry.io/",
          "note": "Use SENTRY_AUTH_TOKEN env to authenticate with Sentry.",
          "project": "divvy",
          "organization": "spicy-box",
          "experimental_android": {
            "enableAndroidGradlePlugin": false
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "c2291691-91be-4a84-a000-c3811e52d28b"
      }
    },
    "owner": "tuanemtramtinh",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/c2291691-91be-4a84-a000-c3811e52d28b"
    },
    "ios": {
      "icon": "./assets/images/logo-app.png"
    }
  }
}
